# editorconfig.org

root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 80

[*.json]
max_line_length = off

[*.md]
max_line_length = off

[browserslist]
max_line_length = off

[*.yml]
max_line_length = off